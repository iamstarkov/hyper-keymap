{
  "name": "hyper-keymap",
  "version": "0.1.0",
  "description": "Hotkeys management for Hyper",
  "main": "src",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "ava",
    "lint": "eslint .",
    "coverage": "nyc ava"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "eslintConfig": {
    "extends": [
      "pedant"
    ],
    "plugins": [
      "require-path-exists"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true
      }
    }
  },
  "nyc": {
    "include": [
      "src/index.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamstarkov/hyper-keymap.git"
  },
  "keywords": [
    "hyper",
    "hyperterm",
    "hyper-plugin",
    "hyperterm-plugin",
    "keymap",
    "hotkey",
    "hotkeys",
    "shortcut",
    "shortcuts",
    "keybinding",
    "kebindings",
    "keymap",
    "keymaps"
  ],
  "author": "Vladimir Starkov <iamstarkov@gmail.com> (https://iamstarkov.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamstarkov/hyper-keymap/issues"
  },
  "homepage": "https://github.com/iamstarkov/hyper-keymap#readme",
  "dependencies": {
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "eslint": "^3.8.1",
    "eslint-config-pedant": "^0.8.0",
    "eslint-plugin-require-path-exists": "^1.1.5",
    "nyc": "^10.2.0"
  }
}
